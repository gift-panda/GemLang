import gem.Util;

class List{
    #init(list, len){
        this.#value = list;
        this.len = len;
    }

    init(list){
        if(!instanceOf(list, List)){
            throw IllegalArgumentError("A List must be passed.");
        }

        this.#value = list.#value;
        this.length = list.length;
    }

    append(value){
        this.#value[this.len] = value;
        this.len = this.len + 1;
        return this;
    }

    length(){
        return this.len;
    }
}